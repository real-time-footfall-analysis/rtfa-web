  "dependencies": {
    "node-sass": "^4.9.3",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-scripts": "2.0.4",
    "prop-types": "latest"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
